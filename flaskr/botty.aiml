  <category>
    <pattern>HELLO</pattern>
    <template>
      Well, hello! I'm Botty!!
    </template>
  </category>
  
  <category>
    <pattern>HI</pattern>
    <template><srai>HELLO</srai></template>
  </category>



  <category>
    <pattern>*</pattern>
    <template>Dunno</template>
  </category>

  <category>
    <pattern>HI MY NAME IS *</pattern>
    <template><think><set name="name"><star/></set></think>Hello <get name="name"/>, I'm Botty. Here to help you choose subjects and courses</template>
  </category>

  <category>
    <pattern>HI I AM *</pattern>
    <template><srai>HI MY NAME IS <star/></srai></template>
  </category>






  <category>
    <pattern>CHOOSE ELECTIVE</pattern>
    <template>Sure. What domain are you interested in?</template> 
  </category>

  <category>
    <pattern>* CHOOSE ELECTIVE *</pattern>
    <template><srai>CHOOSE ELECTIVE</srai></template> 
  </category>

  <category>
    <pattern>* CHOOSE ELECTIVE</pattern>
    <template><srai>CHOOSE ELECTIVE</srai></template> 
  </category>

  <category>
    <pattern>CHOOSE ELECTIVE *</pattern>
    <template><srai>CHOOSE ELECTIVE</srai></template>
  </category>

  <category>
    <pattern>* CHOOSE ELECTIVES *</pattern>
    <template><srai>CHOOSE ELECTIVE</srai></template> 
  </category>

  <category>
    <pattern>* CHOOSE ELECTIVES</pattern>
    <template><srai>CHOOSE ELECTIVE</srai></template> 
  </category>

  <category>
    <pattern>CHOOSE ELECTIVES *</pattern>
    <template><srai>CHOOSE ELECTIVE</srai></template>
  </category>    




  <category>
    <pattern>WHAT SHOULD I DO</pattern>
    <template>What is it that you really want to know?</template>
  </category>

  <category>
    <pattern>I AM CONFUSED</pattern>
    <template>What faculty do you like?</template>
  </category>

  <category>
    <pattern>I DONT KNOW</pattern>
    <template>
      <srai>I AM CONFUSED</srai>
    </template>
  </category>









    






